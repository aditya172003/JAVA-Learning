import java.util.*;

class Question_2
{
	public static void main(String [] args)
	{
	
		/*
			Q 2 ] Scan age and gender from a candidate and check if he/she is able to marry or not.

		*/
		
		
		String  str;
		char    gender;
		int     age;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter your gender m(male) and f(female) :");
		str    = sc.next();
		gender = str.charAt(0);
		
		System.out.println("Enter your age :");
		age    = sc.nextInt();
		
		if(gender == 'm')
		{	if( age >=21)
			{
				System.out.println("YES");
			}
			else
			{
				System.out.println("Wait more for "+(21-age));
			}
			
		} 
		else if ( gender == 'f' )
		{
			if( age >=18)
			{
				System.out.println("YES");
			}
			else
			{
				System.out.println("Wait more for "+(18-age));
			}
		
		}
		else
		{
			System.out.println("Invalid Input");
			
		
		}
			
	
	
	
	}






}
