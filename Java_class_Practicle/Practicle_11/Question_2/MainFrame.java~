import java.awt.*;

class MainFrame extends Frame
{
	MainFrame()
	{
		setSize(600,600);
		setTitle("Question 2");
		setLocationRelativeTo(null);
		setVisible(true);
	}
	
	public void paint(Graphics g)
	{
		g.setColor(Color.RED);
		
		g.fillRect(50,50,200,300);
		
		g.setColor(Color.BLUE);
		
		g.drawOval(250,350,200,200);
	}
}
