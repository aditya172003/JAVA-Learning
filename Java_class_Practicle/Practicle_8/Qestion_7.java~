

class Myclass extends Throwable
{
	String s;
	
	public Myclass(String s)
	{
		super(s);
		this.s =s;
	}
	
	/*
	public String toString()
	{
	
		return s;
	}
	*/
}



class Question_7
{
	public static void main(String [] args)
	{
	
		try
		{
			Myclass M = new Myclass("null error");
			throw(M);
		}
		catch(Myclass A)
		{
			System.out.println(A);
		}
		
	}

}
