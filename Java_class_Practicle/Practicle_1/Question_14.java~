import java.util.*;


class Question_14
{
	public static void main(String [] ar)
	{
	
		int n;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number : ");
		n = sc.nextInt();
		
		// pattern 1 
		System.out.println("pattern 1");
		
		for(int i =1;i<=n;i++)
		{
		
			for(int j =1 ;j<=i;j++)
			{
				System.out.print(j+ " ");
			}
			System.out.println();
		
		}
		
		System.out.println("Enter the number : ");
		n = sc.nextInt();
		
		// pattern 2 
		System.out.println("pattern 2");
		
		for(int i =1;i<=n;i++)
		{
		
			for(int j =1 ;j<=i;j++)
			{
				System.out.print((char)(j+64)+ " ");
			}
			System.out.println();
		
		}
	}

}
