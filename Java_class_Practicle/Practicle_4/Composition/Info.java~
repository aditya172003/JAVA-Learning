


import java.util.Scanner;

class Info
{
	private String str;
	
	public  Info()
	{
		str="";
		
	}
	
	public void getString()
	{
		Scanner sc = new Scanner(System.in);
		
		str = sc.nextLine();
		
	}
	
	public void showString()
	{
		System.out.println(str);
	}
	
	public char getChar(int i)
	{
		return str.charAt(i);
	}
	
}
