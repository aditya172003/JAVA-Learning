import java.util.Scanner;

class Base
{
	int bs;
}
class Power
{
	int pw;
}
class Operation
{
	Base base;
	Power power;
}
class Question_3
{
	public static void  main(String [] args)
	{
		Operation op;
		op         = new Operation();
		op.base    = new Base();
		op.power   = new Power();
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Enter the base");
		op.base.bs = sc.nextInt();
		System.out.println("Enter the power");
		op.power.pw = sc.nextInt();
		int ans = 1;
		for(int i =0;i<op.power.pw;i++)
		{
			ans= ans*op.base.bs;
			
		}
		System.out.println("ans = "+ans);
		

		
		
		//print answer here in main().
	}
}
