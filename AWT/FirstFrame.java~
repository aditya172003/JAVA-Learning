import java.awt.*;


class MyApp1 extends Frame
{

	MyApp1()
	{
		
		
		this.setSize(500,500);  // in the pixel 
		this.setTitle("FirstFrame");
		this.setLocationRelativeTo(null);
		
		this.setVisible(true);
		paint(this.getGraphics());
		
	}
	public void paint(Graphics g )
	{
		
		g.setColor(new Color(255,111,100));
		g.drawLine(200,200,350,200); //(x,y,x,y)
		
		
		g.setColor(new Color(200,20,244));
		
		g.drawString("Aditya Govinda Mahajan",200,200);//(string , x ,y)
		
		
	}
	
	
}


class FirstFrame
{

	public  static void main(String args[])
	{
		MyApp1 frame  = new MyApp1();	
	}
	
}
